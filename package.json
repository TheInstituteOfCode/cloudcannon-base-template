  },
  "homepage": "https://github.com/PollockCR/ioc-jekyll-tailwind#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "glob": "^10.4.1",
